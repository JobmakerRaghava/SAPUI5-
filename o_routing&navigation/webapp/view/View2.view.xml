<mvc:View xmlns:form="sap.ui.layout.form"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core" controllerName="resoucemodel.controller.View2">
    <Page id="page2" showHeader="false" class="sapUiTinyMargin">
        <f:ShellBar id="bar" title="Student Deatils" showNavButton="true" navButtonPressed="onPress"></f:ShellBar>
        <form:SimpleForm id="form2" class="sapUiMediumMargin " editable="true" width="100%">
            <Label id="l1" text="{i18n>ID}" class="text" design="Bold"></Label>
            <Text text="{ID}" id="t1"></Text>
            <Label id="l2" text="{i18n>Name}" design="Bold" class="text"></Label>
            <Text text="{Name}" id="t2" class="text2"></Text>
        </form:SimpleForm>
        <Button id="button1" text="Back" type="Default" class="sapMBtnContent button" press="onPress"></Button>
        <Button id="button2" text="Back" type="Default" class="button2" press="onPress"></Button>
       
    </Page>
</mvc:View>